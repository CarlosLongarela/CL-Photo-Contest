<?xml version="1.0"?>
<ruleset name="Cl-Photo-Contest">
	<description>CL Photo Contest WordPress phpcs Standard.</description>

	<rule ref="WordPress-Core" />
	<rule ref="WordPress-Docs" />
	<rule ref="WordPress-Extra" />

	<rule ref="WordPress.WP.I18n">
		<properties>
			<property name="text_domain" type="array" value="cl-photo-contest" />
		</properties>
	</rule>

  <exclude-pattern>*/node_modules/*</exclude-pattern>
  <exclude-pattern>*/vendor/*</exclude-pattern>
</ruleset>